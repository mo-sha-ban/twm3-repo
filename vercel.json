{
  "version": 2,
  "builds": [
    {
      "src": "twm3-backend/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "twm3-backend/server.js"
    }
  ],
  "includeFiles": [
    "*.html",
    "assets/**",
    "css/**",
    "js/**",
    "img/**",
    "uploads/**",
    "courses.json",
    "products.json",
    "users.json",
    "twm3-backend/public/**"
  ]
}

