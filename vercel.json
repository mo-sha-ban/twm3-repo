{
    "version": 2,
    "builds": [
        {
            "src": "twm3-backend/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "twm3-backend/server.js"
        },
        {
            "src": "/index.html",
            "dest": "/index.html",
            "headers": {
                "Cache-Control": "public, max-age=0, must-revalidate"
            }
        },
        {
            "src": "/css/(.*)",
            "dest": "/css/$1",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/js/(.*)",
            "dest": "/js/$1",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/img/(.*)",
            "dest": "/img/$1",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/(.*)\\.html",
            "dest": "/$1.html",
            "headers": {
                "Cache-Control": "public, max-age=0, must-revalidate"
            }
        }
    ]
}