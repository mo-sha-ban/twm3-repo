{
    "version": 2,
    "builds": [
        {
            "src": "twm3-backend/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "twm3-backend/server.js"
        },
        {
            "src": "/auth/(.*)",
            "dest": "twm3-backend/server.js"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}