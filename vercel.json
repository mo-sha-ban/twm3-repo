{
    "version": 2,
    "functions": {
        "twm3-backend/server.js": {
            "includeFiles": "**/*"
        }
    },
    "builds": [
        {
            "src": "twm3-backend/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(css|js|img|pdfjs|assets|Pages|scripts|uploads)/(.*)",
            "dest": "/$1/$2"
        },
        {
            "src": "/(.*)\\.json",
            "dest": "/$1.json"
        },
        {
            "src": "/(.*)\\.html",
            "dest": "/$1.html"
        },
        {
            "src": "/api/(.*)",
            "dest": "/twm3-backend/server.js"
        },
        {
            "src": "/auth/(.*)",
            "dest": "/twm3-backend/server.js"
        },
        {
            "src": "/(?!api|auth|_vercel|favicon\\.ico).*",
            "dest": "/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "/twm3-backend/server.js"
        }
    ],
    "env": {
        "GOOGLE_CLIENT_ID": "@google-client-id",
        "GOOGLE_CLIENT_SECRET": "@google-client-secret",
        "GITHUB_CLIENT_ID": "@github-client-id",
        "GITHUB_CLIENT_SECRET": "@github-client-secret",
        "JWT_SECRET": "@jwt-secret",
        "MONGO_URI": "@mongo-uri",
        "FRONTEND_BASE_URL": "https://twm3.org",
        "NODE_ENV": "production"
    },
    "cleanUrls": true,
    "trailingSlash": false
}